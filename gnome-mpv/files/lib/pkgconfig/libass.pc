prefix=/app
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: libass
Description: LibASS is an SSA/ASS subtitles rendering library
Version: 0.13.1
Requires: 
Requires.private: harfbuzz >= 0.9.5, fontconfig >= 2.10.92, fribidi >= 0.19.0, freetype2 >= 9.10.3
Libs: -L${libdir} -lass 
Libs.private: -lm
Cflags: -I${includedir}
